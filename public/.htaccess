<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /2022/react/
    RewriteRule ^/2022/react/index.html$ - [L]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-l
    RewriteRule ^(.*) /2022/react/index.html [NC,L]
  </IfModule>
  
  
